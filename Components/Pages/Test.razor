@page "/Test"
@rendermode InteractiveServer
<h3>Test</h3>

<InputText @bind-Value="@Objects["work"]"/>
<button @onclick=" OnSubmit">Submit</button>


@code {
 private Dictionary<string, string> Objects { get; set; } = new();
 protected override void OnInitialized()
 {
  base.OnInitialized();
  Objects.Add("work", "none");
 }
 private void OnSubmit()
 {
  Console.WriteLine(Objects["work"]);
 }

}